{
  "name": "@oakfinch/ts-utils",
  "version": "1.0.9",
  "description": "Utility library with enhanced TypeScript support",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "rm -rf dist; tsc --build tsconfig.json tsconfig.esm.json",
    "build:docs": "typedoc",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "pre-push": "npm run lint; npm run typecheck; npm run test; npm run build:docs; git add ./docs/*; git commit --amend --no-edit || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oakfinch/ts-utils.git"
  },
  "keywords": [
    "TypeScript"
  ],
  "author": "Nate Volker <natevolker@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oakfinch/ts-utils/issues"
  },
  "homepage": "https://github.com/oakfinch/ts-utils#readme",
  "devDependencies": {
    "@oakfinch/eslint-config": "^1.0.4",
    "@oakfinch/jest-config": "^1.0.1",
    "@oakfinch/ts-extra": "^1.0.13",
    "@oakfinch/tsconfig": "^1.0.7",
    "@types/jest": "^27.0.2",
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "ts-jest": "^27.0.5",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.3"
  },
  "dependencies": {}
}
